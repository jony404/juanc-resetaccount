{
	"$schema": "http://json-schema.org/draft-04/schema#",
	"type": "object",
	"properties": {
		"message": {
			"type": "string"
		},
		"data": {
			"type": "object",
			"properties": {
				"validateUserAccountResponse": {
					"type": "object",
					"properties": {
						"passwordReset": {
							"type": "object",
							"properties": {
								"success": {
									"type": "boolean"
								},
								"newPassword": {
									"type": "string"
								}
							}
						},
						"accountUnblock": {
							"type": "object",
							"properties": {
								"success": {
									"type": "boolean"
								}
							}
						}
					},
					"oneOf": [{
							"required": ["passwordReset"]
						},
						{
							"required": ["accountUnblock"]
						}
					]
				}
			}
		}
	},
	"required": [
		"message",
		"data"
	]
}